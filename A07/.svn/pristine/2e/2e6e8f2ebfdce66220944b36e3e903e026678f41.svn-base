#include <iostream>
#include "complex.hpp"

int main()
{
  Complex c1;
  Complex c2;

  c1.setComplex(1.145,-0.865);
  c2.setComplex(0.444,-2.0);

  c1.printComplex();
  c2.printComplex();

  Complex c3;

  c3=c1+c2;
  c3.printComplex();
  c3=c1-c2;
  c3.printComplex();
  c3=c1*c2;
  c3.printComplex();
  c3=c1/c2;
  c3.printComplex();

  return 0;
}

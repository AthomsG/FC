#include <iostream>
using namespace std;

int main()
{
  double sum = 0;
  for(double x=0;x<=5.5;x+=0.1){
    sum+=x;
    cout << "x = " << x << endl;
  }
  cout << "Sum = " << sum << endl;
  return 0;
}

// nao funciona com x!=5.5 pois os numeros do sistema de ponto flutuante
// nao sao representados com precisao total
